#ALIASES
alias python="python3"
alias e="explorer.exe"
alias pip="pip3"
alias ..='cd ..'
alias rm="trash"
alias vi="vim"
alias mi="micro"
alias ls="lsd"
alias zs="source ~/.zshrc"
alias batcat="bat"
alias zerotier="sudo zerotier-cli"
alias ifconfig="ip addr show"

if command -v code
then
  alias c="code"
  alias code="code"
elif command -v code-insiders
then
  alias c="code-insiders"
  alias code="code-insiders"
fi
